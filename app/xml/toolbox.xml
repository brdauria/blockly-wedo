  <xml id="toolbox">
    <!--<category id="cat01" name="Start">
      <block type="when_start_clicked"></block>
    </category>-->
    <category id="cat02" name="Outputs" colour="#5ba5a5">
      <block type="wedo_turn_on"></block>
      <block type="wedo_turn_off"></block>
      <block type="wedo_everything_off"></block>
      <block type="wedo_turn_on_for">
        <value name="time">
          <block type="number">
            <field name="value">2</field>
          </block>
        </value>
      </block>
      <block type="wedo_power">
        <value name="power">
          <block type="number" id="17">
            <field name="value">50</field>
          </block>
        </value>
      </block>
      <block type="wedo_motor_direction"></block>
      <block type="led_colour"></block>
	  <block type="piezo">
        <value name="time">
          <block type="number">
            <field name="value">100</field>
          </block>
        </value>
      </block>     
    </category>
    <category id="cat03" name="Inputs" colour="#7c5385">
      <block type="wedo_if">
        <field name="comp">&lt;</field>
        <value name="input">
          <block type="sensor">
            <field name="sensor">A</field>
          </block>
        </value>
        <value name="value">
          <block type="number">
            <field name="value">10</field>
          </block>
        </value>
      </block>
      <block type="wedo_if_else">
        <field name="comp">&lt;</field>
        <value name="input">
          <block type="sensor">
            <field name="sensor">A</field>
          </block>
        </value>
        <value name="value">
          <block type="number">
            <field name="value">10</field>
          </block>
        </value>
      </block>
      <block type="wedo_if_tilt">
        <value name="input">
          <block type="sensor">
            <field name="sensor">A</field>
          </block>
        </value>
      </block>
      <block type="wedo_if_tilt_else">
        <value name="input">
          <block type="sensor">
            <field name="sensor">A</field>
          </block>
        </value>
      </block>
      <block type="sensor"></block>
    </category>
    <category id="cat04" name="Delays" colour="#49a6d4">
      <block type="wait">
        <value name="time">
          <block type="number">
            <field name="value">1</field>
          </block>
        </value>
      </block>
      <block type="wait_until">
        <field name="comp">&lt;</field>
        <value name="input">
          <block type="sensor">
            <field name="sensor">A</field>
          </block>
        </value>
        <value name="value">
          <block type="number">
            <field name="value">10</field>
          </block>
        </value>
      </block>
      <block type="wait_until_tilt">
        <value name="input">
          <block type="sensor">
            <field name="sensor">A</field>
          </block>
        </value>
      </block>
      <block type="wait_until_button"></block>     
    </category>
    <category id="cat05" name="Loops" colour="#77ab41">
      <block type="repeat"></block>
      <block type="repeat_count">
        <value name="count">
          <block type="number">
            <field name="value">3</field>
          </block>
        </value>
      </block>
      <block type="repeat_until">
        <field name="comp">&lt;</field>
        <value name="input">
          <block type="sensor">
            <field name="sensor">A</field>
          </block>
        </value>
        <value name="value">
          <block type="number">
            <field name="value">10</field>
          </block>
        </value>
      </block>
      <block type="repeat_until_tilt">
        <value name="input">
          <block type="sensor">
            <field name="sensor">A</field>
          </block>
        </value>
      </block>
    </category>
    <category id="cat06" name="Variables" colour="#d05f2d">
      <block type="var_set">
        <field name="var">varA</field>
        <value name="value">
          <block type="number">
            <field name="value">10</field>
          </block>
        </value>
      </block>
      <block type="var_random"></block>
      <block type="var_inc">
        <value name="value">
          <block type="number">
            <field name="value">3</field>
          </block>
        </value>
      </block>
      <block type="var_dec">
        <value name="value">
          <block type="number">
            <field name="value">3</field>
          </block>
        </value>
      </block>
      <block type="variable"></block>
      <block type="string"></block>
      <block type="number"></block>
    </category>
    <category id="cat07" name="Computer" colour="#cc0000">
      <block type="comment"></block>
      <block type="log_clear"></block>
      <block type="log">
        <value name="value">
          <block type="string">
            <field name="value">Hello world!</field>
          </block>
        </value>
      </block>
      <block type="log">
        <value name="value">
          <block type="variable">
            <field name="var">varA</field>
          </block>
        </value>
      </block>
      <block type="log">
        <value name="value">
          <block type="sensor">
            <field name="sensor">A</field>
          </block>
        </value>
      </block>
      <block type="play_file">
        <field name="value">0001.MP3</field>
      </block>
      <block type="play_stop"></block>      
    </category>
	<!-- <category id="cat08" name="Procedures" custom="PROCEDURE" colour="#7c5385"></category> -->
</xml>
