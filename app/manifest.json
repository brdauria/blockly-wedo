{
  "name": "BlocklyWeDo",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Blockly for WeDo",
  "minimum_chrome_version": "41",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "res/brick-16.png", "24": "res/brick-24.png", "128": "res/brick-128.png" },
  "bluetooth": {
      "low_energy": true,
      "uuids": ["1800", "1801", "180a", "180f","00001523-1212-efde-1523-785feabcd123", "00004f0e-1212-efde-1523-785feabcd123"]
  },
  "permissions": [
    "hid",
    "serial",
    "storage",   
    {
      "fileSystem": [
        "write"
      ]
    },
    {
      "usbDevices": [
	    { "vendorId": 1684, "productId": 3 }
 	  ]
	}
  ]
}
