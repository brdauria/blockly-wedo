{
  "name": "BlocklyWeDo",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Blockly for WeDo",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "res/brick-16.png", "24": "res/brick-24.png", "128": "res/brick-128.png" },
  "sockets": {
    "tcpServer": { "listen" : [":17301", ":17307"] }
  },
  "permissions": [
    "app.window.alwaysOnTop",
    "hid",
    "serial",
    "storage",
    {
      "fileSystem": [
        "write"
      ]
    },
    {
			"usbDevices": [
				{ "vendorId": 1684, "productId": 3 }
				,{ "vendorId": 1240, "productId": 63 }
 			]
		}
  ]
}
